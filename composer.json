{
    "name": "sujan/phppackage",
    "description": "Simpole package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mdsujansarkar",
            "email": "sujansarkar786@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.20"
    }
}
